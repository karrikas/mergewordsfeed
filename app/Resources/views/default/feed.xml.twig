<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
	xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
	>
	<channel>
		<title>My feed</title>
		<link>http://karrikas.com</link>
		<description>karrikas</description>
		<atom:link href="http://feed.karrikas.com/feed/{{ feedId }}.xml" rel="self" type="application/rss+xml" />
		{% for item in feed %}
		<item>
			<title>Tweet</title>
			<pubDate>{{ item.date }}</pubDate>
			<guid isPermaLink="false">http://yocato.com/{{item.md5}}</guid>
			<link>http://yocato.com/{{item.md5}}</link>
			<description><![CDATA[{{ item.content }}]]></description>
		</item>
		{% endfor %}
	</channel>
</rss>
